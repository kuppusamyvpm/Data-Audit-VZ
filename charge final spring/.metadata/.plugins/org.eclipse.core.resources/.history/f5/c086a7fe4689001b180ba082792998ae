package com.charge.demo.controller;

import java.util.List;

import org.springframework.web.bind.annotation.CrossOrigin;
import org.springframework.web.bind.annotation.GetMapping;

import com.charge.demo.model.charge;
import com.charge.demo.model.overallprojection;
import com.charge.demo.service.overallprojectionService;

public class overallprojectionController
{
	private overallprojectionService os;
	@GetMapping("/ccabs")
    @CrossOrigin(origins="http://localhost:4200")
	   public List<overallprojection> getchargebyrawquery3()
    {
        return os.getchargebyrawquery3();
    }

}
