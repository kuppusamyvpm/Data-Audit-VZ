package com.charge.demo.model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="COMP_CHARGE")
public class charge
{
	@Id
	 String  ACCOUNT_NAME ;
	 int BILLING_PERIOD;                           
	 String  BILL_TYPE ;                                                        
	 float CCABS_CHARGE;                            
	 float SV_CHARGE;                              
	 float DIFF_CHARGE;                              
	 float PERC_DIFF;
	 int RECORD_TYPE_ID;
	 public charge()
	 {
		 
	 }
	 public charge(String aCCOUNT_NAME, int bILLING_PERIOD, String bILL_TYPE, float cCABS_CHARGE, float sV_CHARGE,
			 float dIFF_CHARGE, float pERC_DIFF,int rECORD_TYPE_ID) {
		super();
		ACCOUNT_NAME = aCCOUNT_NAME;
		BILLING_PERIOD = bILLING_PERIOD;
		BILL_TYPE = bILL_TYPE;
		CCABS_CHARGE = cCABS_CHARGE;
		SV_CHARGE = sV_CHARGE;
		DIFF_CHARGE = dIFF_CHARGE;
		PERC_DIFF = pERC_DIFF;
	}
	 

	public String getACCOUNT_NAME() {
		return ACCOUNT_NAME;
	}
	public void setACCOUNT_NAME(String aCCOUNT_NAME) {
		ACCOUNT_NAME = aCCOUNT_NAME;
	}
	public int getBILLING_PERIOD() {
		return BILLING_PERIOD;
	}
	public void setBILLING_PERIOD(int bILLING_PERIOD) {
		BILLING_PERIOD = bILLING_PERIOD;
	}
	public String getBILL_TYPE() {
		return BILL_TYPE;
	}
	public void setBILL_TYPE(String bILL_TYPE) {
		BILL_TYPE = bILL_TYPE;
	}
	public float getCCABS_CHARGE() {
		return CCABS_CHARGE;
	}
	public void setCCABS_CHARGE(float cCABS_CHARGE) {
		CCABS_CHARGE = cCABS_CHARGE;
	}
	public float getSV_CHARGE() {
		return SV_CHARGE;
	}
	public void setSV_CHARGE(float sV_CHARGE) {
		SV_CHARGE = sV_CHARGE;
	}
	public float getDIFF_CHARGE() {
		return DIFF_CHARGE;
	}
	public void setDIFF_CHARGE(float dIFF_CHARGE) {
		DIFF_CHARGE = dIFF_CHARGE;
	}
	public float getPERC_DIFF() {
		return PERC_DIFF;
	}
	public void setPERC_DIFF(float pERC_DIFF) {
		PERC_DIFF = pERC_DIFF;
	}
	 public int getRECORD_TYPE_ID() {
		return RECORD_TYPE_ID;
	}
	public void setRECORD_TYPE_ID(int rECORD_TYPE_ID) {
		RECORD_TYPE_ID = rECORD_TYPE_ID;
	}
	                              	 
}
