package com.charge.demo.model;

import java.util.Map;

import javax.persistence.Id;

public class ccabs_charge {
	@Id
	 String  ACCOUNT_NAME ;
	 int BILLING_PERIOD;                           
	 String  BILL_TYPE ;                                                        
	 String CCABS_CHARGE;                            
	 String SV_CHARGE;                              
	 String DIFF_CHARGE;                              
	 String PERC_DIFF;
	 int RECORD_TYPE_ID;
	 public ccabs_charge()
	 {
		 
	 }
	 public ccabs_charge(String aCCOUNT_NAME, int bILLING_PERIOD, String bILL_TYPE, String cCABS_CHARGE, String sV_CHARGE,
			 String dIFF_CHARGE, String pERC_DIFF,int rECORD_TYPE_ID) {
		super();
		ACCOUNT_NAME = aCCOUNT_NAME;
		BILLING_PERIOD = bILLING_PERIOD;
		BILL_TYPE = bILL_TYPE;
		CCABS_CHARGE = cCABS_CHARGE;
		SV_CHARGE = sV_CHARGE;
		DIFF_CHARGE = dIFF_CHARGE;
		PERC_DIFF = pERC_DIFF;
	}
	 

	public String getACCOUNT_NAME() {
		return ACCOUNT_NAME;
	}
	public void setACCOUNT_NAME(String aCCOUNT_NAME) {
		ACCOUNT_NAME = aCCOUNT_NAME;
	}
	public int getBILLING_PERIOD() {
		return BILLING_PERIOD;
	}
	public void setBILLING_PERIOD(int bILLING_PERIOD) {
		BILLING_PERIOD = bILLING_PERIOD;
	}
	public String getBILL_TYPE() {
		return BILL_TYPE;
	}
	public void setBILL_TYPE(String bILL_TYPE) {
		BILL_TYPE = bILL_TYPE;
	}
	public String getCCABS_CHARGE() {
		return CCABS_CHARGE;
	}
	public void setCCABS_CHARGE(String cCABS_CHARGE) {
		CCABS_CHARGE = cCABS_CHARGE;
	}
	public String getSV_CHARGE() {
		return SV_CHARGE;
	}
	public void setSV_CHARGE(String sV_CHARGE) {
		SV_CHARGE = sV_CHARGE;
	}
	public String getDIFF_CHARGE() {
		return DIFF_CHARGE;
	}
	public void setDIFF_CHARGE(String dIFF_CHARGE) {
		DIFF_CHARGE = dIFF_CHARGE;
	}
	public String getPERC_DIFF() {
		return PERC_DIFF;
	}
	public void setPERC_DIFF(String pERC_DIFF) {
		PERC_DIFF = pERC_DIFF;
	}
	 public int getRECORD_TYPE_ID() {
		return RECORD_TYPE_ID;
	}
	public void setRECORD_TYPE_ID(int rECORD_TYPE_ID) {
		RECORD_TYPE_ID = rECORD_TYPE_ID;
	}

	
		
}
