package com.charge.demo.controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.CrossOrigin;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

import com.charge.demo.model.overall_projection;
import com.charge.demo.repository.overall_projection_repo;
import com.charge.demo.service.overall_projection_service;

@RestController
@CrossOrigin("*")
public class overall_projection_controller
{
	@Autowired
	private overall_projection_repo opr;
	
	@Autowired
	private overall_projection_service ops;
	
	  @GetMapping("/overallprojection")
	    @CrossOrigin(origins="http://localhost:4200")
		   public List<overall_projection> getchargebyrawquery3()
	    {
	        return ops.getchargebyrawquery3();
	    }

	
}
